vcare:CKDSimulation
  rdf:type vcare:ClinicalPathwaySimulation ;
  vcare:hasObservationFeature vcare:A1C ;
  vcare:hasObservationFeature vcare:ACR ;
  vcare:hasObservationFeature vcare:BMI ;
  vcare:hasObservationFeature vcare:DiastolicValue ;
  vcare:hasObservationFeature vcare:Hematuria ;
  vcare:hasObservationFeature vcare:Kalium ;
  vcare:hasObservationFeature vcare:Potassium ;
  vcare:hasObservationFeature vcare:SystolicValue ;
  vcare:hasObservationFeature vcare:eGFR ;
  vcare:hasSPARQLQuery """PREFIX vcare: <http://vcare.eu/vcare#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?state ?reward ?condition ?expr ?feat ?fType ?unit ?vecsize ?activity
WHERE {
    ?simulation rdf:type vcare:SimulationTask.
    ?simulation vcare:simulatesUseCase vcare:CKDPathway.
    ?uc vcare:hasState ?state.
    ?state vcare:hasReward ?reward.
    OPTIONAL {?state vcare:hasCondition ?condition.
        OPTIONAL {?condition vcare:hasConditionExpression ?expr.}
        ?condition vcare:hasObservationFeature ?feat.
        ?feat vcare:featureType ?fType.
        OPTIONAL{?feat vcare:hasUnit ?unit.}
        OPTIONAL {?feat vcare:hasVectorSize ?vecsize.}
    }
    OPTIONAL {?uc vcare:requiresActivity ?activity.}
}""" ;
  vcare:hasSPARQLQuery """PREFIX vcare: <http://vcare.eu/vcare#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?state ?reward ?condition ?expr ?feat ?fType ?unit ?vecsize ?activity
WHERE {
    vcare:CKDPathway vcare:hasState ?state.
    ?state vcare:hasReward ?reward.
    OPTIONAL {?state vcare:hasCondition ?condition.
        OPTIONAL {?condition vcare:hasConditionExpression ?expr.}
        ?condition vcare:hasObservationFeature ?feat.
        ?feat vcare:featureType ?fType.
        OPTIONAL{?feat vcare:hasUnit ?unit.}
        OPTIONAL {?feat vcare:hasVectorSize ?vecsize.}
    }
    OPTIONAL {?uc vcare:requiresActivity ?activity.}
}""" ;
  vcare:simulatesUseCase vcare:CKDPathway ;
  rdfs:label "vcare CKDSimulation" ;
.
