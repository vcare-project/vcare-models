vcare:ActivityCount
  rdf:type sh:SPARQLConstraint ;
  rdfs:label "Activity count" ;
  sh:select """PREFIX vcare: <http://vcare.eu/vcare#>
SELECT $this  (COUNT(?activity) as ?activitycount)
WHERE { 
$this vcare:simulatesUseCase ?uc.
?uc vcare:requiresActivity ?activity.
} GROUP BY $this""" ;
